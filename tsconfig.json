{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "lib": ["es7", "es2017", "es2018", "es2019", "es2020", "esnext","dom"],
    "target": "es6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false
  }
}
